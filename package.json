{
  "name": "@adobe/reactor-babel-plugin-replace-tokens-edge",
  "version": "1.1.2",
  "description": "Babel plugin that will replace data element tokens (eg: {{some_data_element}}) with valid JavaScript.",
  "main": "src/index.js",
  "author": {
    "name": "Adobe Systems",
    "url": "http://adobe.com",
    "email": "reactor@adobe.com"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:adobe/reactor-babel-plugin-replace-tokens-edge.git"
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint 'src/**'",
    "debug": "reactor-token-scripts-edge sanitize --filePath test/fixtures/container.js | node --inspect ./node_modules/.bin/babel --no-babelrc --plugins=./src/index.js",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix"
    ]
  },
  "dependencies": {
    "@adobe/reactor-token-scripts-edge": "^1.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/generator": "^7.15.8",
    "@babel/parser": "^7.15.8",
    "@babel/traverse": "^7.15.4",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.2",
    "jest": "^27.2.5",
    "jest-runner-eslint": "^0.11.1",
    "lint-staged": "^11.2.3",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.1"
  },
  "license": "Apache-2.0"
}
